. ~/autoload/path_variable

alias l='command ls --color=auto --group-directories-first'
alias ls='l -lhA --time-style long-iso'
alias cdm='cd `ls -t | head -n 1`'
alias clip='xclip -selection c'
alias gs='git status'
alias gd='git diff'
alias gdc='git diff --cached'
alias gl='git log'
alias ga='git add -A'
alias gc='git commit -m '
alias gf='git fetch --all'
alias gr='git rebase'
alias pym='python3 -m'
alias np='ipython -i -c " \
  import numpy as np; \
  import matplotlib.pyplot as plt"'
alias tf='ipython -i -c " \
  import numpy as np; \
  import matplotlib.pyplot as plt; \
  import tensorflow as tf"'
alias cmc='rm CMakeCache.txt && rm -rf CMakeFiles'
alias ll='tree -aL 2'
alias remove_broken_links='find -xtype l -delete'
alias tfpython='LD_PRELOAD=/usr/lib/libtcmalloc.so python'
alias tmux='tmux -f ~/.config/tmux/tmux.conf'
